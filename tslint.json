{
  "rules": {
    "indent": [
      true,
      "spaces"
    ],
    "whitespace": [
      true,
      "check-branch",
      "check-operator",
      "check-separator"
    ],
    "no-trailing-whitespace": true,
    "no-trailing-comma": true,
    "quotemark": [
      true,
      "single"
    ],
    "semicolon": true,
    "no-console": true,
    "class-name": true,
    "curly": true,
    "triple-equals": [
      true,
      "allow-null-check"
    ]
  }
}