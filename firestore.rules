rules_version = '2';
service cloud.firestore {
  match /databases/{database}/documents {
    match /{document=**} {
      allow read, write: if false;
    }

    match /base/{document} {
      allow read: if true;
      allow write: if false;
    }

    match /layout/{document} {
      allow read: if true;
      allow write: if false;
    }

    match /alliance/{document} {
      allow read: if true;
      allow write: if false;
    }
  }
}
